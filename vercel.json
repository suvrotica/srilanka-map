{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "zeroConfig": true }
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/.*", "dest": "/index.html" }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Permissions-Policy",
            "value": "attribution-reporting=(), run-ad-auction=(), join-ad-interest-group=(), compute-pressure=(), browsing-topics=()"
          }
        ]
      }
    ]
  }